<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="progressForegroundColor"
        format="color"/>
    <attr name="progressBackgroundColor"
        format="color"/>

    <declare-styleable name="ColorfulCircleIndicator">
        <attr name="arrowSrc"
            format="reference"/>
        <attr name="arrowRotate"
            format="integer"/>
        <attr name="progressForegroundColor"/>
        <attr name="progressBackgroundColor"/>
        <attr name="progressBackgroundWidth"
            format="dimension"/>
        <attr name="progressForegroundWidth"
            format="dimension"/>
        <attr name="progressBackgroundStartAngle"
            format="integer"/>
        <attr name="progressBackgroundSweepAngle"
            format="integer"/>
        <attr name="progressForegroundStartAngle"
            format="integer"/>
        <attr name="progressForegroundSweepAngle"
            format="integer"/>
        <attr name="progressArcPadding"
            format="dimension"/>
        <attr name="outermostCircleRadius"
            format="dimension"/>
    </declare-styleable>

</resources>