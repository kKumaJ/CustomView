<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorfulCircleIndicator">
        <attr name="arrowSrc" format="reference"/>
        <attr name="arrowRotate" format="integer"/>
        <attr name="progressBackgroundWidth" format="dimension"/>
        <attr name="progressBackgroundColor" format="color"/>
        <attr name="progressForegroundWidth" format="dimension"/>
        <attr name="progressForegroundColor" format="string"/>
        <attr name="progressBackgroundStartAngle" format="integer"/>
        <attr name="progressBackgroundSweepAngle" format="integer"/>
        <attr name="progressForegroundStartAngle" format="integer"/>
        <attr name="progressForegroundSweepAngle" format="integer"/>
        <attr name="progressArcPadding" format="dimension"/>
        <attr name="outermostCircleRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="OctoCatView">
        <attr name="strokeWidth" format="dimension"/>
        <attr name="color" format="color"/>
        <attr name="colos" format="reference"/>
        <attr name="section" format="integer"/>
        <attr name="intervalAngle" format="float"/>
        <attr name="style" format="reference"/>
    </declare-styleable>
</resources>